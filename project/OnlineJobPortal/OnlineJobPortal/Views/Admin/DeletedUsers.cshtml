@model IEnumerable<OnlineJobPortal.Models.register>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_AdminLayout";
    ViewBag.title = "Manage Users";
}

<h1>Manage Deleted Users </h1>

<table class="table table-bordered">
    <tr>
        <th>Sr </th>
        <th>Name </th>
        <th>Email </th>
        <th>Password </th>
        <th>Mobile </th>
        <th>Address</th>
        <th>Restore</th>
    </tr>

    @{
        int sr = 1;
    }
    @foreach (var data in Model)
    {
        <tr>
            <td> @sr </td>
            <td> @data.name </td>
            <td> @data.email </td>
            <td> @data.password </td>
            <td> @data.mobile </td>
            <td> @data.address </td>
            <td>
                <a class="btn btn-success" asp-action="RestoreUser" asp-route-id="@data.id">Restore</a>
            </td>
        </tr>

        sr++;
    }


</table>